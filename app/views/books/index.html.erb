<h1>Books</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
  </tr>
  <% @books.each do |book| %>
  <tr>
    <td><%= book.title %></td>
    <td><%= book.body %></td>
    <td><a href="">Show</a></td>
    <td><a href="">Edit</a></td>
    <td><a href="">Destroy</a></td>
  </tr>
  <% end %>
</table>

